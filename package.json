{
  "name": "epluskit",
  "type": "module",
  "version": "0.0.3",
  "description": "基于 Element Plus 的扩展与工具库。",
  "author": {
    "name": "joygqz",
    "email": "joygqz@gmail.com",
    "url": "https://github.com/joygqz"
  },
  "license": "MIT",
  "homepage": "https://github.com/joygqz/epluskit#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/joygqz/epluskit.git"
  },
  "bugs": {
    "url": "https://github.com/joygqz/epluskit/issues"
  },
  "keywords": [
    "element-plus",
    "vue3",
    "kit",
    "ui",
    "components",
    "utilities",
    "plugins",
    "extensions",
    "vue",
    "typescript",
    "vite"
  ],
  "sideEffects": [
    "*.css",
    "*.less",
    "*.scss",
    "element-plus/es/components/**/style/css"
  ],
  "directories": {
    "dist": "./dist",
    "es": "./es",
    "example": "./example"
  },
  "exports": {
    ".": {
      "types": "./es/index.d.ts",
      "import": "./es/index.js",
      "require": "./lib/index.js"
    },
    "./global": "./es/global.d.ts",
    "./*": "./*"
  },
  "main": "./lib/index.js",
  "module": "./es/index.js",
  "types": "./es/index.d.ts",
  "files": [
    "CHANGELOG.md",
    "LICENSE",
    "README-zh-CN.md",
    "README.md",
    "dist",
    "es",
    "lib"
  ],
  "engines": {
    "node": ">=20.0.0",
    "npm": ">=8.0.0"
  },
  "browserslist": [
    "es6",
    "not dead",
    "Chrome >= 51",
    "Firefox >= 54",
    "Safari >= 10",
    "Edge >= 79",
    "and_chr >= 51",
    "and_ff >= 54",
    "ios_saf >= 10.3"
  ],
  "scripts": {
    "dev": "run-p example \"build:es {@}\" -- --watch",
    "example": "vite --config vite.serve.config.ts",
    "build": "run-s lint build:es build:dist",
    "build:es": "vite build --config vite.build.config.ts",
    "build:dist": "vite build --mode dist --config vite.build.config.ts",
    "lint": "run-p lint:*",
    "lint:eslint": "eslint --fix ./packages",
    "lint:typescript": "vue-tsc -b",
    "patch": "npm version patch -m 'chore: release v%s'",
    "prepublishOnly": "pnpm build",
    "release": "npm publish --access public",
    "release:dry": "npm publish --dry-run"
  },
  "peerDependencies": {
    "typescript": ">=5.0.0"
  },
  "peerDependenciesMeta": {
    "typescript": {
      "optional": true
    }
  },
  "devDependencies": {
    "@antfu/eslint-config": "^5.2.0",
    "@microsoft/api-extractor": "^7.52.10",
    "@types/node": "^24.2.1",
    "@vitejs/plugin-vue": "^6.0.1",
    "@vue/language-core": "^3.0.5",
    "@vue/tsconfig": "^0.7.0",
    "element-plus": "^2.10.6",
    "eslint": "^9.33.0",
    "jiti": "^2.5.1",
    "npm-run-all2": "^8.0.4",
    "typescript": "~5.8.3",
    "unplugin-auto-import": "^20.0.0",
    "unplugin-dts": "1.0.0-beta.6",
    "unplugin-vue-components": "^29.0.0",
    "vite": "^7.1.0",
    "vue": "^3.5.18",
    "vue-tsc": "^3.0.5"
  }
}

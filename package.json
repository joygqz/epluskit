{
  "name": "epluskit",
  "type": "module",
  "version": "0.0.4",
  "description": "基于 Element Plus 的扩展与工具库。",
  "author": {
    "name": "joygqz",
    "email": "joygqz@gmail.com",
    "url": "https://github.com/joygqz"
  },
  "license": "MIT",
  "homepage": "https://github.com/joygqz/epluskit#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/joygqz/epluskit.git"
  },
  "bugs": {
    "url": "https://github.com/joygqz/epluskit/issues"
  },
  "keywords": [
    "element-plus",
    "vue3",
    "kit",
    "ui",
    "components",
    "utilities",
    "plugins",
    "extensions",
    "vue",
    "typescript",
    "vite"
  ],
  "sideEffects": [
    "**/*.css"
  ],
  "directories": {
    "dist": "./dist",
    "es": "./es",
    "example": "./example"
  },
  "exports": {
    ".": {
      "types": "./es/index.d.ts",
      "import": "./es/index.js",
      "require": "./lib/index.js"
    },
    "./global": "./es/global.d.ts",
    "./*": "./*"
  },
  "main": "./lib/index.js",
  "module": "./es/index.js",
  "unpkg": "./dist/index.js",
  "jsdelivr": "./dist/index.js",
  "style": "dist/index.css",
  "types": "./es/index.d.ts",
  "files": [
    "CHANGELOG.md",
    "LICENSE",
    "README-zh-CN.md",
    "README.md",
    "dist",
    "es",
    "lib"
  ],
  "engines": {
    "node": ">=20.0.0",
    "npm": ">=8.0.0"
  },
  "browserslist": [
    "es6",
    "not dead",
    "Chrome >= 51",
    "Firefox >= 54",
    "Safari >= 10",
    "Edge >= 79",
    "and_chr >= 51",
    "and_ff >= 54",
    "ios_saf >= 10.3"
  ],
  "scripts": {
    "dev": "run-p serve \"build:module {@}\" -- --watch",
    "serve": "run-s build:module example",
    "example": "vite --config vite.serve.config.ts",
    "build": "run-s lint typecheck build:module build:dist",
    "build:module": "vite build --config vite.build.config.ts",
    "build:dist": "vite build --mode dist --config vite.build.config.ts",
    "lint": "eslint --fix",
    "typecheck": "vue-tsc --build",
    "prepack": "pnpm build",
    "prepare": "simple-git-hooks",
    "release": "bumpp && pnpm publish"
  },
  "peerDependencies": {
    "@onlyoffice/document-editor-vue": ">=1.6.0",
    "element-plus": ">=2.10.0",
    "typescript": ">=5.0.0"
  },
  "peerDependenciesMeta": {
    "typescript": {
      "optional": true
    }
  },
  "devDependencies": {
    "@antfu/eslint-config": "^5.2.1",
    "@microsoft/api-extractor": "^7.52.10",
    "@onlyoffice/document-editor-vue": "^1.6.0",
    "@types/node": "^24.2.1",
    "@types/path-browserify": "^1.0.3",
    "@vitejs/plugin-vue": "^6.0.1",
    "@vue/language-core": "^3.0.5",
    "@vue/tsconfig": "^0.7.0",
    "bumpp": "^10.2.3",
    "element-plus": "^2.10.6",
    "eslint": "^9.33.0",
    "jiti": "^2.5.1",
    "npm-run-all2": "^8.0.4",
    "sass-embedded": "^1.90.0",
    "simple-git-hooks": "^2.13.1",
    "typescript": "~5.8.3",
    "unplugin-auto-import": "^20.0.0",
    "unplugin-dts": "1.0.0-beta.6",
    "unplugin-vue": "^7.0.1",
    "unplugin-vue-components": "^29.0.0",
    "vite": "^7.1.2",
    "vue": "^3.5.18",
    "vue-tsc": "^3.0.5"
  },
  "simple-git-hooks": {
    "pre-commit": "npx lint-staged"
  },
  "lint-staged": {
    "*": "eslint --fix"
  }
}
